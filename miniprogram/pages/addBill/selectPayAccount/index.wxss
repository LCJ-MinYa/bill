page {
    background: #f5f5f5;
}

.container {
    background: #f5f5f5;
    height: auto !important;
}

.no-account-box {
    width: 100%;
    position: relative;
    display: flex;
    flex: 1;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 40% 0;
}

.no-account-box>view {
    font-size: 28rpx;
    color: #dedede;
}

.icon-meiyoushuju {
    padding-bottom: 30rpx;
    font-size: 100rpx !important;
}

.account-list-box {
    position: relative;
    overflow: hidden;
    width: 100%;
    padding: 0 40rpx 120rpx;
}

.account-item-box {
    position: relative;
    overflow: hidden;
    width: 100%;
    padding: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    border-radius: 15rpx;
    margin-bottom: 30rpx;
}

.account-item-box>view.iconfont {
    font-size: 40rpx;
}

.icon-weixinzhifu {
    color: rgba(9, 187, 7, 1);
}

.icon-zhifubao {
    color: rgba(2, 169, 241, 1);
}

.icon-yinhangqia1 {
    font-size: 36rpx !important;
    color: rgba(250, 65, 65, 1);
}

.account-left-box {
    display: flex;
    align-items: center;
}

.account-left-box>view.name {
    font-size: 28rpx;
    color: #666;
    padding-left: 20rpx;
}

.account-left-box>view.detail {
    font-size: 24rpx;
    color: #666;
}
.row-center {
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #fff;
}

.row-space-between {
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
}

.row-flex-start {
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    background: #fff;
}

.row-flex-end {
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    background: #fff;
}

.row-space-between {
    width: 100%;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
}

.column-center {
    width: 100%;
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    background: #fff;
}

.column-cross-flex-start {
    width: 100%;
    position: relative;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: center;
    background: #fff;
}

.placeholder-center {
    display: flex;
    align-items: center;
}

page {
    height: 100%;
}

.container {
    width: 100%;
    height: 100%;
    background: #FFF;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.size-box {
    position: relative;
    width: 100%;
    background: #F4F4F4;
    height: 20rpx;
}

steps,
confirmButton,
van-cell-group,
van-cell {
    width: 100%;
}

.van-cell {
    padding-top: 30rpx !important;
    padding-bottom: 30rpx !important;
}

.van-cell__left-icon-wrap {
    margin-right: 15rpx !important;
}














